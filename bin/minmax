#!/usr/bin/awk -f

function tally(value)
{
    min = (value < min ? value : min)
    max = (value > max ? value : max)
}
BEGIN {
    min = max = 0
    if (ARGC > 1) {
	for (i = 1; i <= ARGC; ++i) {
	    tally(ARGV[i])
	}
	exit 0
    }
}
{
    for (i = 1; i <= NF; ++i) {
	tally($i)
    }
}
END {
    printf "%d %d\n", min, max
}
